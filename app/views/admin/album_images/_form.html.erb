<%= form_with(model: [:admin, @album_image], local: true ) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title, class: "form-control" %>

  <%= form.file_field :image, id: :album_image %>
  <br>
  <%= image_tag("", id: "preview", style: "display: none; height: 200px; with: 200px") %>
  <br>

  <%= form.submit %>
<% end %>
<%= javascript_pack_tag 'album_load_image'%>
